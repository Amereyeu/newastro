---
import Layout from "../layout/default.astro";
import mainImage from "../images/post-1.jpg";
---

<Layout>
  <section class="cta">
    <h1 class="cta__header">Home page</h1>
    <img
      src={mainImage.src}
      alt="main image"
      class="cta__image"
      loading="eager"
      decoding="async"
    />
  </section>
</Layout>
